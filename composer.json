{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-redis": "*",
        "anthropic-ai/sdk": "^0.3.0",
        "api-platform/core": "^4.2.9",
        "beberlei/doctrineextensions": "^1.5",
        "doctrine/dbal": "^4.4.1",
        "doctrine/doctrine-bundle": "^3.1",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5.8",
        "dompdf/dompdf": "^3.1.4",
        "dragonmantank/cron-expression": "^3.6",
        "easycorp/easyadmin-bundle": ">=4.27.4",
        "endroid/qr-code-bundle": ">=6.1",
        "gedmo/doctrine-extensions": "dev-main || >=3.21",
        "knplabs/knp-paginator-bundle": "^6.10.0",
        "league/commonmark": "^2.8.0",
        "lexik/jwt-authentication-bundle": "^3",
        "nyholm/psr7": "^1.8.2",
        "openai-php/client": "^0.18.0",
        "phpdocumentor/reflection-docblock": "^5.6.5",
        "phpoffice/phpspreadsheet": "^5.3",
        "phpstan/phpdoc-parser": "^2.3",
        "roave/security-advisories": "dev-master",
        "scheb/2fa-bundle": ">=8",
        "scheb/2fa-totp": ">=8",
        "sentry/sentry-symfony": "^5.8.2",
        "symfony/apache-pack": "^1.0.1",
        "symfony/asset": "^8.0",
        "symfony/asset-mapper": "^8.0",
        "symfony/console": "^8.0.1",
        "symfony/doctrine-messenger": "^8.0.1",
        "symfony/dotenv": "^8.0",
        "symfony/expression-language": "^8.0",
        "symfony/flex": "^2.10.0",
        "symfony/form": "^8.0.1",
        "symfony/framework-bundle": "^8.0.1",
        "symfony/html-sanitizer": "^8.0",
        "symfony/http-client": "^8.0.1",
        "symfony/intl": "^8.0.1",
        "symfony/mailer": "^8.0",
        "symfony/mime": "^8.0",
        "symfony/monolog-bundle": "^4.0.1",
        "symfony/notifier": "^8.0",
        "symfony/polyfill-intl-messageformatter": "^1.33",
        "symfony/polyfill-php83": "^1.33",
        "symfony/process": "^8.0",
        "symfony/property-access": "^8.0",
        "symfony/property-info": "^8.0.1",
        "symfony/rate-limiter": "^8.0",
        "symfony/redis-messenger": "^8.0",
        "symfony/runtime": "^8.0.1",
        "symfony/scheduler": "^8.0",
        "symfony/security-bundle": "^8.0",
        "symfony/security-csrf": "^8.0",
        "symfony/serializer": "^8.0.2",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "^8.0.1",
        "symfony/translation": "^8.0.1",
        "symfony/twig-bundle": "^8.0",
        "symfony/ux-live-component": "^2.31",
        "symfony/ux-turbo": "^2.31",
        "symfony/ux-twig-component": "^2.31",
        "symfony/validator": "^8.0.2",
        "symfony/var-exporter": "^8.0",
        "symfony/web-link": "^8.0",
        "symfony/webpack-encore-bundle": "^2.4",
        "symfony/yaml": "^8.0.1",
        "twig/extra-bundle": "^2.12|^3.22.1",
        "twig/intl-extra": "^3.22.1",
        "twig/markdown-extra": "^3.22",
        "twig/string-extra": "^3.22.1",
        "twig/twig": "^2.12|^3.22.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.4.1",
        "dbrekelmans/bdi": "^1.4.1",
        "doctrine/doctrine-fixtures-bundle": "^4.3.1",
        "fakerphp/faker": "^1.24.1",
        "friendsofphp/php-cs-fixer": "^3.91.3",
        "infection/infection": "dev-master",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.33",
        "phpstan/phpstan-doctrine": "^2.0.12",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0.9",
        "phpunit/phpunit": "^12.5.3",
        "qossmic/deptrac": "^2.0",
        "squizlabs/php_codesniffer": "^3.10",
        "symfony/browser-kit": "^8.0",
        "symfony/css-selector": "^8.0",
        "symfony/debug-bundle": "^8.0",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/stopwatch": "^8.0",
        "symfony/web-profiler-bundle": "^8.0",
        "zenstruck/foundry": "^2.8.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true,
            "phpstan/extension-installer": true,
            "infection/extension-installer": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "phpstan analyse --configuration=phpstan.neon --memory-limit=512M",
        "phpstan-clear": "phpstan clear-result-cache",
        "phpcsfixer": "php-cs-fixer fix --dry-run --diff --ansi",
        "phpcsfixer-fix": "php-cs-fixer fix --ansi",
        "phpcs": "phpcs --standard=phpcs.xml",
        "phpcbf": "phpcbf --standard=phpcs.xml",
        "check-code": [
            "@phpcsfixer",
            "@phpstan",
            "@phpcs"
        ],
        "check-architecture": "deptrac analyze --config-file=deptrac.yaml --no-interaction",
        "check-all": [
            "@check-code",
            "@check-architecture"
        ],
        "test": "phpunit -c phpunit.xml.dist",
        "test-unit": "phpunit --testsuite unit",
        "test-functional": "phpunit --testsuite functional",
        "test-integration": "phpunit --testsuite integration",
        "test-api": "phpunit --testsuite api",
        "test-security": "phpunit --testsuite security",
        "test-e2e": "phpunit --testsuite e2e",
        "infection": "infection --threads=4 --min-msi=70 --min-covered-msi=80 --show-mutations --no-interaction",
        "infection-ci": "infection --threads=max --min-msi=70 --min-covered-msi=80 --logger-github --no-interaction",
        "infection-debug": "infection --threads=1 --min-msi=70 --min-covered-msi=80 --show-mutations --debug",
        "deptrac": "deptrac analyze --config-file=deptrac.yaml --no-interaction",
        "deptrac-graph": "deptrac analyze --config-file=deptrac.yaml --formatter=graphviz --output=var/deptrac/dependencies.svg",
        "deptrac-debug": "deptrac analyze --config-file=deptrac.yaml --formatter=console -vvv",
        "security-check": "composer audit",
        "security-test": [
            "@security-check",
            "@test-security"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^8.0",
            "docker": true
        }
    }
}
